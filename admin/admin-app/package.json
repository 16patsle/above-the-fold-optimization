{
  "name": "admin-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@wordpress/components": "^13.0.0",
    "debounce-promise": "^3.1.2",
    "jsoneditor": "^8.6.3",
    "monaco-editor": "^0.20.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
